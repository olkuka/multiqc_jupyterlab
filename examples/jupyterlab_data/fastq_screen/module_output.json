n                fq_screen_dict[dict.plot_id] = dict;\n            });\n\n            $(function () {\n                // In case of repeated modules: #fq_screen_plot, #fq_screen_plot-1, ..\n                $(\".fq_screen_plot\").each(function () {\n                    var plot_id = $(this).attr('id');\n\n                    $(this).highcharts({\n                        chart: { type: \"column\", backgroundColor: null },\n                        title: { text: \"FastQ Screen Results\" },\n                        xAxis: { categories: fq_screen_dict[plot_id].categories },\n                        yAxis: {\n                            max: 100,\n                            min: 0,\n                            title: { text: \"Percentage Aligned\" }\n                        },\n                        tooltip: {\n                            formatter: function () {\n                                return \"<b>\" + this.series.stackKey.replace(\"column\",\"\") + \" - \" + this.x + \"</b><br/>\" +\n                                    this.series.name + \": \" + this.y + \"%<br/>\" +\n                                    \"Total Alignment: \" + this.point.stackTotal + \"%\";\n                            },\n                        },\n                        plotOptions: {\n                            column: {\n                                pointPadding: 0,\n                                groupPadding: 0.02,\n                                stacking: \"normal\"\n                            }\n                        },\n                        series: fq_screen_dict[plot_id].data\n                    });\n                });\n            });\n        </script>", "print_section": true}], "anchor": "fastq_screen", "name": "FastQ Screen", "intro": "<p><a href=\"http://www.bioinformatics.babraham.ac.uk/projects/fastq_screen/\" target=\"_blank\">FastQ Screen</a> allows you to screen a library of sequences in FastQ format against a set of sequence databases so you can see if the composition of the library matches with what you expect.<em class=\"text-muted small\" style=\"margin-left: 1rem;\">DOI:  <a class=\"module-doi\" data-doi=\"10.12688/f1000research.15931.2\" data-toggle=\"popover\" href=\"https://doi.org/10.12688/f1000research.15931.2\" target=\"_blank\">10.12688/f1000research.15931.2</a>.</em></p>"}